
public class Class2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			double meters = 33000;
			double kilometers = meters / 1000;
			double milimeters = meters * 1000000;
			double feet = meters * 3.28;
			double inch = meters * 39.37;
			
			System.out.println (meters + "[m]");
			System.out.printf ("%.0f = [km] \n", kilometers);
			System.out.printf ("%.0f = [mm] \n", milimeters);
			System.out.printf ("%.0f = [feet] \n", feet);
			System.out.printf ("%.0f = [inch] \n", inch);
			
					
			
			
			
			
	}

}
