package ba.bitcamp.edvin.vjezbe;

public class Zadatak3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 57;
		int b = 55;
		int c = 3;
		if ( a>b && a>c){
		System.out.println(a);
	} else if (b>a && b>c){
		System.out.println(b);
		
	}else if (c > a && c>b){
		System.out.println(c);
	}
		
	}

}
