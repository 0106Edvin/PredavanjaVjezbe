package ba.bitcamp.edvin.vjezbe;

public class Zadaci7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int day = 20;
		int month = 1;
		int date = day+1;
		
		if (date < 32 && month<13){
			System.out.println("Sutra je " + date + " " + month);
		}else System.out.println("Datum netacan");
		

	}

}
